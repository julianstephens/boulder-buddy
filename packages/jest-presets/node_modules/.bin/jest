#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/julian/workspace/boulder-buddy/node_modules/.pnpm/jest@29.6.4_@types+node@18.16.0/node_modules/jest/bin/node_modules:/home/julian/workspace/boulder-buddy/node_modules/.pnpm/jest@29.6.4_@types+node@18.16.0/node_modules/jest/node_modules:/home/julian/workspace/boulder-buddy/node_modules/.pnpm/jest@29.6.4_@types+node@18.16.0/node_modules:/home/julian/workspace/boulder-buddy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/julian/workspace/boulder-buddy/node_modules/.pnpm/jest@29.6.4_@types+node@18.16.0/node_modules/jest/bin/node_modules:/home/julian/workspace/boulder-buddy/node_modules/.pnpm/jest@29.6.4_@types+node@18.16.0/node_modules/jest/node_modules:/home/julian/workspace/boulder-buddy/node_modules/.pnpm/jest@29.6.4_@types+node@18.16.0/node_modules:/home/julian/workspace/boulder-buddy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/jest@29.6.4_@types+node@18.16.0/node_modules/jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/jest@29.6.4_@types+node@18.16.0/node_modules/jest/bin/jest.js" "$@"
fi
