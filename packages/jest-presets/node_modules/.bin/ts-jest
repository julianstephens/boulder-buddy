#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/julian/workspace/boulder-buddy/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@7.22.17_jest@29.6.4_typescript@4.9.5/node_modules/ts-jest/node_modules:/home/julian/workspace/boulder-buddy/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@7.22.17_jest@29.6.4_typescript@4.9.5/node_modules:/home/julian/workspace/boulder-buddy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/julian/workspace/boulder-buddy/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@7.22.17_jest@29.6.4_typescript@4.9.5/node_modules/ts-jest/node_modules:/home/julian/workspace/boulder-buddy/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@7.22.17_jest@29.6.4_typescript@4.9.5/node_modules:/home/julian/workspace/boulder-buddy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
