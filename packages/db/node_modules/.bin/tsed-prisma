#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/julian/workspace/boulder-buddy/node_modules/.pnpm/@tsed+prisma@7.35.1_@prisma+client@5.2.0_@tsed+core@7.35.1_@tsed+di@7.35.1_@tsed+json-mapper@7.35.1_@tsed+schema@7.35.1/node_modules/@tsed/prisma/lib/cjs/node_modules:/home/julian/workspace/boulder-buddy/node_modules/.pnpm/@tsed+prisma@7.35.1_@prisma+client@5.2.0_@tsed+core@7.35.1_@tsed+di@7.35.1_@tsed+json-mapper@7.35.1_@tsed+schema@7.35.1/node_modules/@tsed/prisma/lib/node_modules:/home/julian/workspace/boulder-buddy/node_modules/.pnpm/@tsed+prisma@7.35.1_@prisma+client@5.2.0_@tsed+core@7.35.1_@tsed+di@7.35.1_@tsed+json-mapper@7.35.1_@tsed+schema@7.35.1/node_modules/@tsed/prisma/node_modules:/home/julian/workspace/boulder-buddy/node_modules/.pnpm/@tsed+prisma@7.35.1_@prisma+client@5.2.0_@tsed+core@7.35.1_@tsed+di@7.35.1_@tsed+json-mapper@7.35.1_@tsed+schema@7.35.1/node_modules/@tsed/node_modules:/home/julian/workspace/boulder-buddy/node_modules/.pnpm/@tsed+prisma@7.35.1_@prisma+client@5.2.0_@tsed+core@7.35.1_@tsed+di@7.35.1_@tsed+json-mapper@7.35.1_@tsed+schema@7.35.1/node_modules:/home/julian/workspace/boulder-buddy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/julian/workspace/boulder-buddy/node_modules/.pnpm/@tsed+prisma@7.35.1_@prisma+client@5.2.0_@tsed+core@7.35.1_@tsed+di@7.35.1_@tsed+json-mapper@7.35.1_@tsed+schema@7.35.1/node_modules/@tsed/prisma/lib/cjs/node_modules:/home/julian/workspace/boulder-buddy/node_modules/.pnpm/@tsed+prisma@7.35.1_@prisma+client@5.2.0_@tsed+core@7.35.1_@tsed+di@7.35.1_@tsed+json-mapper@7.35.1_@tsed+schema@7.35.1/node_modules/@tsed/prisma/lib/node_modules:/home/julian/workspace/boulder-buddy/node_modules/.pnpm/@tsed+prisma@7.35.1_@prisma+client@5.2.0_@tsed+core@7.35.1_@tsed+di@7.35.1_@tsed+json-mapper@7.35.1_@tsed+schema@7.35.1/node_modules/@tsed/prisma/node_modules:/home/julian/workspace/boulder-buddy/node_modules/.pnpm/@tsed+prisma@7.35.1_@prisma+client@5.2.0_@tsed+core@7.35.1_@tsed+di@7.35.1_@tsed+json-mapper@7.35.1_@tsed+schema@7.35.1/node_modules/@tsed/node_modules:/home/julian/workspace/boulder-buddy/node_modules/.pnpm/@tsed+prisma@7.35.1_@prisma+client@5.2.0_@tsed+core@7.35.1_@tsed+di@7.35.1_@tsed+json-mapper@7.35.1_@tsed+schema@7.35.1/node_modules:/home/julian/workspace/boulder-buddy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/ts-node" ]; then
  exec "$basedir/ts-node"  "$basedir/../@tsed/prisma/lib/cjs/generator.js" "$@"
else
  exec ts-node  "$basedir/../@tsed/prisma/lib/cjs/generator.js" "$@"
fi
